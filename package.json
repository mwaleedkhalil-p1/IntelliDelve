{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:django": "python manage.py runserver 8080",
    "build": "npm run build:client",
    "build:client": "vite build",
    "start": "python manage.py runserver 8080",
    "start:prod": "gunicorn backend.wsgi:application --bind 0.0.0.0:8080",
    "django:migrate": "python manage.py migrate",
    "django:createsuperuser": "python manage.py createsuperuser",
    "django:collectstatic": "python manage.py collectstatic --noinput",
    "django:setup": "pip install -r requirements.txt && python manage.py migrate && python manage.py collectstatic --noinput",
    "vercel-build": "npm install && npm run build && npm run django:setup",
    "test": "vitest --run",
    "test:django": "python manage.py test",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
